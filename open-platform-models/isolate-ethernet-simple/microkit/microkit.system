  <protection_domain name="seL4_ArduPilot_ArduPilot_MON" domain="domain_4">
    <program_image path="seL4_ArduPilot_ArduPilot_MON.elf" />
    <protection_domain name="seL4_ArduPilot_ArduPilot" domain="domain_4" id="1" stack_size="0x100_000" smc="true">
      <program_image path="seL4_ArduPilot_ArduPilot.elf" />
      <map mr="ZCU102_Impl_Instance_seL4_ArduPilot_ArduPilot_RxQueueFree_1_Memory_Region"
           vaddr="0x10_000_000"
           perms="rw"
           setvar_vaddr="RxQueueFree_queue_1"
      />
      <map mr="ZCU102_Impl_Instance_seL4_ArduPilot_ArduPilot_TxQueueAvail_1_Memory_Region"
           vaddr="0x10_001_000"
           perms="rw"
           setvar_vaddr="TxQueueAvail_queue_1"
      />
      <map mr="ZCU102_Impl_Instance_seL4_RxFirewall_RxFirewall_RxOutQueueAvail_1_Memory_Region"
           vaddr="0x10_002_000"
           perms="r"
           setvar_vaddr="RxQueueAvail_queue_1"
      />
      <map mr="ZCU102_Impl_Instance_seL4_TxFirewall_TxFirewall_TxInQueueFree_1_Memory_Region"
           vaddr="0x10_003_000"
           perms="r"
           setvar_vaddr="TxQueueFree_queue_1"
      />
      <map mr="ZCU102_Impl_Instance_seL4_LowLevelEthernetDriver_LowLevelEthernetDriver_RxData_1_Memory_Region"
           vaddr="0x10_004_000"
           perms="r"
           setvar_vaddr="RxData_queue_1"
      />
      <map mr="ZCU102_Impl_Instance_seL4_ArduPilot_ArduPilot_TxData_1_Memory_Region"
           vaddr="0x10_036_000"
           perms="rw"
           setvar_vaddr="TxData_queue_1"
      />
      <map mr="guest_ram" vaddr="0x8_0000_0000" perms="rw" setvar_vaddr="guest_ram_vaddr" />
        <virtual_machine name="linux">
            <vcpu id="0" />
            <map mr="guest_ram" vaddr="0x8_0000_0000" perms="rwx" />
            <map mr="serial" vaddr="0xff000000" perms="rw" cached="false" />
            <map mr="mmc" vaddr="0xff170000" perms="rw" cached="false" />
            <map mr="gic_vcpu" vaddr="0xf9020000" perms="rw" cached="false" />
        </virtual_machine>
        <!-- UART interrupt -->
        <irq irq="53" id="1" />
        <!-- MMC interrupt -->
        <irq irq="81" id="2" />
    </protection_domain>
  </protection_domain>


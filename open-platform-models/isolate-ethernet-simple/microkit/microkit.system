    1 - pacer
    2 - eth_driver
    3 - rx_firewall
    4 - VM
    5 - tx_firewall
    6 - placeholder
  -->
  <!-- length in microseconds -->
  <domain_schedule>
    <domain name="domain_1" length="30" /> <!-- Seems to be the min of pacer -->
    <domain name="domain_4" length="600" />
    <!-- <domain name="domain_4" length="5000" /> -->
    <domain name="domain_1" length="30" />
    <domain name="domain_5" length="200" />
    <domain name="domain_1" length="30" />
    <domain name="domain_3" length="70" />
    <domain name="domain_1" length="30" />
    <domain name="domain_2" length="300" />
    <domain name="domain_1" length="30" />
    <domain name="domain_3" length="200" />
    <domain name="domain_1" length="30" />
